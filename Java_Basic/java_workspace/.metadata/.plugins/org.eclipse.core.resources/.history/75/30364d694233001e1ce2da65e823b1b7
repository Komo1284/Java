package test05;

public class Ex03 {
	public static void main(String[] args) {
		
		long var1 = 10;
		long var2 = 20L;
		long var3 = 1000000000000;
		long var4 = 1000000000000L; //값자체를 자바에서는 Literal 이라고한다.
		
		System.out.println(var1);
		System.out.println(var2);
		System.out.println(var4);
	}

}
